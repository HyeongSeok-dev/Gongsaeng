<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.gongsaeng.mapper.CartMapper">

	<!-- 장바구니에 물건넣기 -->
	<insert id="insertCart">
		INSERT
		INTO cart
		VALUES(
			#{cart_idx} 
			, #{member_id}
			, #{class_idx}
		);
	</insert>
	
</mapper>